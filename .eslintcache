onents\\myForm.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\myFormTest.jsx",["145"],"import React, { Component } from \"react\";\r\nimport DateTimePicker from \"react-datetime-picker\";\r\nimport { Redirect } from \"react-router-dom\";\r\nimport { saveJournal } from \"./../services/journalService\";\r\nimport { toast } from \"react-toastify\";\r\n\r\nclass MyFormTest extends Component {\r\n  state = {\r\n    data: {\r\n      date: new Date(),\r\n      comment: \"\",\r\n      locked: false,\r\n      starred: false,\r\n    },\r\n  };\r\n\r\n  componentDidMount() {\r\n    if (this.props.comment) {\r\n      const { comment, date } = this.props;\r\n\r\n      const data = { ...this.state.data };\r\n      data[\"comment\"] = comment;\r\n      data[\"date\"] = new Date(date);\r\n      this.setState({ data });\r\n    }\r\n  }\r\n\r\n  handleChange = e => {\r\n    const data = { ...this.state.data };\r\n    data[\"comment\"] = e.currentTarget.value;\r\n    this.setState({ data });\r\n  };\r\n\r\n  handleDateChange = value => {\r\n    const data = { ...this.state.data };\r\n    data[\"date\"] = value;\r\n    this.setState({ data });\r\n  };\r\n\r\n  handleSave = async () => {\r\n    const { comment, date } = this.state.data;\r\n    if (comment === \"\") return toast.error(\"Field can't be empty\");\r\n    if (!date) return toast.error(\"Invalide date.\");\r\n    console.log(date);\r\n\r\n    let { data: journal } = this.props._id // this means we are using it from the journals page\r\n      ? await saveJournal({ _id: this.props._id, ...this.state.data })\r\n      : await saveJournal(this.state.data); // means it's a new entry\r\n\r\n    console.log(journal);\r\n    if (this.props.onSave) this.props.onSave(this.state.data);\r\n    // after Editing... props from card\r\n    else this.props.history.push(`/journals/${journal._id}`);\r\n  };\r\n\r\n  handleCancel = () => {\r\n    if (this.props.onSave) this.props.onSave(null);\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <div className=\"container\">\r\n        <div className=\"form-group d-flex flex-column align-items-center m-2\">\r\n          <label htmlFor=\"form\">How was your day?</label>\r\n          <textarea\r\n            autoFocus\r\n            value={this.state.data[\"comment\"]}\r\n            onChange={this.handleChange}\r\n            className=\"form-control mb-3\"\r\n            id=\"form\"\r\n            rows=\"10\"\r\n          ></textarea>\r\n          <div className=\"w-100 mb-2\">\r\n            <DateTimePicker\r\n              minDate={new Date(\"1900\")}\r\n              maxDate={new Date(\"2100\")}\r\n              required={true}\r\n              onChange={this.handleDateChange}\r\n              value={this.state.data[\"date\"]}\r\n            />\r\n          </div>\r\n          <div className=\"w-100\">\r\n            <button className=\"btn btn-primary m-2\" onClick={this.handleSave}>\r\n              Save\r\n            </button>\r\n            <button className=\"btn btn-danger m-2\" onClick={this.handleCancel}>\r\n              Cancel\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default MyFormTest;\r\n","C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\optionsDropdown.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\lockModal.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\box.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\random.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\utils\\formatDate.js",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\loginForm.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\registerForm.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\input.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\lea