s.onSave(null);\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <div className=\"container\">\r\n        <div className=\"form-group d-flex flex-column align-items-center m-2\">\r\n          <label htmlFor=\"form\">How was your day?</label>\r\n          <textarea\r\n            autoFocus\r\n            value={this.state.data[\"comment\"]}\r\n            onChange={this.handleChange}\r\n            className=\"form-control mb-3\"\r\n            id=\"form\"\r\n            rows=\"10\"\r\n          ></textarea>\r\n          <div className=\"w-100 mb-2\">\r\n            <DateTimePicker\r\n              minDate={new Date(\"1900\")}\r\n              maxDate={new Date(\"2100\")}\r\n              required={true}\r\n              onChange={this.handleDateChange}\r\n              value={this.state.data[\"date\"]}\r\n            />\r\n          </div>\r\n          <div className=\"w-100\">\r\n            <button className=\"btn btn-primary m-2\" onClick={this.handleSave}>\r\n              Save\r\n            </button>\r\n            <button className=\"btn btn-danger m-2\" onClick={this.handleCancel}>\r\n              Cancel\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default MyFormTest;\r\n","C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\optionsDropdown.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\lockModal.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\box.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\random.jsx",["146"],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\utils\\formatDate.js",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\loginForm.jsx",["147"],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\registerForm.jsx",[],"C:\\Users\\AymaneHR\\Desktop\\code\\react\\learning\\src\\components\\input.jsx",[],