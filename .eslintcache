nth: value });\r\n    console.log(value.getMonth());\r\n  };\r\n\r\n  render() {\r\n    const { journals } = this.state;\r\n    return (\r\n      <React.Fragment>\r\n        <Container className=\"mt-2\">\r\n          <Row>\r\n            <Col>\r\n              <div className=\"sticky-top p-2\">\r\n                <div>\r\n                  <p>Filter By</p>\r\n                  <input\r\n                    className=\"mr-1\"\r\n                    type=\"radio\"\r\n                    name=\"sorting\"\r\n                    id=\"day\"\r\n                    onChange={e => this.handleFilter(e)}\r\n                    checked={this.state.filter === \"day\"}\r\n                  />\r\n                  <label className=\"pr-3\" htmlFor=\"day\">\r\n                    Day\r\n                  </label>\r\n                  <input\r\n                    className=\"mr-1\"\r\n                    type=\"radio\"\r\n                    name=\"sorting\"\r\n                    id=\"month\"\r\n                    onChange={e => this.handleFilter(e)}\r\n                    checked={this.state.filter === \"month\"}\r\n                  />\r\n                  <label className=\"pr-3\" htmlFor=\"month\">\r\n                    Month\r\n                  </label>\r\n                  <input\r\n                    className=\"mr-1\"\r\n                    type=\"radio\"\r\n                    name=\"sorting\"\r\n                    id=\"search\"\r\n                    onChange={e => this.handleFilter(e)}\r\n                    checked={this.state.filter === \"search\"}\r\n                  />\r\n                  <label htmlFor=\"search\">Search</label>\r\n                </div>\r\n                <Filter\r\n                  pickedMonth={this.state.pickedMonth}\r\n                  onChange={this.handlePick}\r\n                  filter={this.state.filter}\r\n                />\r\n              </div>\r\n            </Col>\r\n            <Col sm={8}>\r\n              {journals\r\n  